    "react-dom": "16.13.1",
    "rsuite": "4.6.0"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/yargs": "17.0.10",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "parcel-bundler": "1.12.4",
    "sass": "1.53.0",
    "ts-node": "8.10.1",
    "typescript": "4.7.4",
    "yargs": "17.5.1"
  },
  "browserslist": [
    "current node",
    "defaults"
  ]
