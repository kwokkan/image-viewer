    "@types/yargs": "17.0.20",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "parcel-bundler": "1.12.5",
    "sass": "1.57.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yargs": "17.6.2"
  },
  "browserslist": [
    "current node",
    "defaults"
  ]
